<% layout("/layouts/boilerplate") -%>
<body>
    <div class="card">
        <img src=<%=item.image%> alt="">
        <div class="title">
            <p><%=item.title%></p>
        </div>
        <div class="desc">
            <p><%=item.description%></p>
        </div>
        <div class="price">Price: &#8377;<%= item.price.toLocaleString("en-IN") %></div>
    </div>
</body>